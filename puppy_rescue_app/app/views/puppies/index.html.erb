<h1>Puppies#index</h1>
<p>Find me in app/views/puppies/index.html.erb</p>
<p><%= link_to "Add a New Pup!", new_puppy_path %></p>

<% @puppies.each do |puppy| %>
    
    <img src="<%= puppy.image_url %>" width = "200px" height = "200px">
    <%= puppy.name %>
    <%= link_to "show", puppy_path(puppy) %>
    
	
<% end %>
